{
  "extends": "tslint:latest",
  "rules": {
    "semicolon": false,
    "quotemark": [true, "single"],
    "no-console": false,
    "no-namespace": false,
    "no-unused-expressions": false,
    "interface-name": false,
    "arrow-parens": false,
    "one-line": false,
    "switch-default": false,
    "variable-name": [true, "allow-leading-underscore"],
    "no-string-throw": false
  }
}
