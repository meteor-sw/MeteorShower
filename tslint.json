{
  "extends": "tslint:latest",
  "rules": {
    "arrow-parens": false,
    "trailing-comma": [true, {
      "multiline": {
        "functions": "never"
      }
    }],
    "curly": [true, "ignore-same-line"],
    "no-console": false,
    "no-namespace": false,
    "no-string-throw": false,
    "interface-name": false,
    "one-line": false,
    "quotemark": [true, "single"],
    "semicolon": false,
    "switch-default": false,
    "variable-name": [true, "allow-leading-underscore"]
  }
}
